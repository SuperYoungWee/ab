local counter = 1

local CrystalPath = game.workspace["MineSS"]:GetChildren()

  for k, v in pairs(CrystalPath) do
      if v.Name == "SovereignTier" then
          v.Name = "Crystal" .. counter
          counter = counter + 1
      end
  end

	for k, v in pairs(CrystalPath) do
      if v.Name == "GodTier" then
          v.Name = "Crystal" .. counter
          counter = counter + 1
      end
  end
