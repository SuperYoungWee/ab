local Condition = true -- true turns it on, false turns it off
local counter = 1

while Condition do
  local ginsengPlants = game.workspace["MineSS"]:GetChildren()

  for k, v in pairs(ginsengPlants) do
      if v.Name == "SovereignTier" then
          v.Name = "MoonieChan" .. counter
          counter = counter + 1
      end
  end

  for i = 1, 16 do
      local mooniechannnn = game.workspace["MineSS"]["MoonieChan" .. i]
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = mooniechannnn.CFrame
      wait(0.3)
      local args = { mooniechannnn, "Mine 10" }
      game:GetService("ReplicatedStorage").Harvest.Harvest:FireServer(unpack(args))
      wait(2)
  end
end

local ginsengPlants1 = game.workspace["MineSS"]:GetChildren()

  for k, v in pairs(ginsengPlants1) do
      if v.Name == "GodTier" then
          v.Name = "MoonieChan1" .. counter
          counter = counter + 1
      end
  end

  for i = 1, 16 do
      local mooniechannnn1 = game.workspace["MineSS"]["MoonieChan1" .. i]
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = mooniechannnn1.CFrame
      wait(0.3)
      local args = { mooniechannnn1, "Mine 9" }
      game:GetService("ReplicatedStorage").Harvest.Harvest:FireServer(unpack(args))
      wait(2)
  end
end
