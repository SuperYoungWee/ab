_G.Condition = true -- true turns it on, false turns it off


while _G.Condition == true do
  local ginsengPlants = game.workspace["MineSS"]:GetChildren()

  for k, v in pairs(ginsengPlants) do
      if v.Name == "SovereignTier" then
          v.Name = "Sov" .. k
		end
	end		
		  
	for i = 1, 16 do
      local Sovv = game.workspace["MineSS"]["Sov" .. i]
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Sovv.CFrame
	  wait(0.3)
      local args = { Sovv, "Mine 10" }
      game:GetService("ReplicatedStorage").Harvest.Harvest:FireServer(unpack(args))
	  wait(2)
	end	  
	    
		  
  for k, v in pairs(ginsengPlants) do
	if v.Name == "GodTier" then
        v.Name = "God" .. k
      end
  end

  for i = 1, 17 do
      local Godd = game.workspace["MineSS"]["God" .. i]
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Godd.CFrame
	  wait(0.3)
      local args = { Godd, "Mine 9" }
      game:GetService("ReplicatedStorage").Harvest.Harvest:FireServer(unpack(args))
	  wait(2)
  end
  
	wait()
  end
